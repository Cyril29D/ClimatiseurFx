<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="sio.d3.javafx.climatiseurfx.HelloController"
    stylesheets="@css/style.css" styleClass="fond">

    <HBox alignment="CENTER_LEFT" spacing="20.0" stylesheets="@css/style.css" styleClass="fond2">
        <Label fx:id="marque" text="marque :" />
        <TextArea fx:id="valMarque" minHeight="16" maxHeight="30" minWidth="69" maxWidth="100"
                  />
    </HBox>

    <HBox alignment="CENTER_LEFT" spacing="20.0" stylesheets="@css/style.css" styleClass="fond1">
        <Label fx:id="puissance" text="Puissance :" />
        <TextArea fx:id="valPuissance" minHeight="16" maxHeight="30" minWidth="69" maxWidth="100"
                  />

    </HBox>

    <Button onMouseClicked="#enregistrer" fx:id="primaryButton"  text="Enregistrer"
            stylesheets="@css/style.css" styleClass="fond3"/>



    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"  />
    </padding>

</VBox>

